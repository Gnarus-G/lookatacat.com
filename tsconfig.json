{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "importsNotUsedAsValues": "error",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "plugins": [
      {
        "name": "typescript-svelte-plugin"
      }
    ]
  }
}
